     <TDFORMAT>/(</TDFORMAT>
     <TDLINE>DATA(lo_progress) = zcl_llm_00_slice_progress=&gt;new_for_table(</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>/(</TDFORMAT>
     <TDLINE>  it_ = lt_data</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>/(</TDFORMAT>
     <TDLINE>  iv_text = &apos;Processing Data&apos;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>/(</TDFORMAT>
